{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/SkillArc",
      "destination": "/SkillArc/index.html"
    },
    {
      "source": "/SkillArc/(.*)",
      "destination": "/SkillArc/$1"
    },
    {
      "source": "/Admin",
      "destination": "/Admin/index.html"
    },
    {
      "source": "/Admin/(.*)",
      "destination": "/Admin/$1"
    }
  ]
}